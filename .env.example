# WebAuthn Configuration
# The domain name (ID) for WebAuthn.
# Localhost: "localhost" or a mapped domain like "192.168.1.5.sslip.io"
# Production: "photos.yourdomain.com"
APP_DOMAIN=localhost

# The full origin URL.
# Localhost: "http://localhost:8080" 
# Production: "https://photos.yourdomain.com"
APP_ORIGIN=http://localhost:8080
